{
  "name": "@alexzzz/pim",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@7.5.1",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/alexzhang1030/pim#readme",
  "bugs": {
    "url": "https://github.com/alexzhang1030/pim/issues"
  },
  "license": "MIT",
  "author": "alexzhang1030",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/pim.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "pim": "./dist/index.mjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && npm publish --access public",
    "test": "vitest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@alexzzz/nl": "^0.0.3",
    "inquirer": "^9.0.0",
    "object-path": "^0.11.8"
  },
  "devDependencies": {
    "@alexzzz/eslint-config": "1.3.0",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/inquirer": "8.2.1",
    "@types/node": "17.0.45",
    "@types/object-path": "0.11.1",
    "bumpp": "8.2.1",
    "changelogithub": "0.12.4",
    "eslint": "8.19.0",
    "fast-license": "1.0.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "pnpm": "7.5.1",
    "tsx": "3.8.0",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "unplugin-auto-import": "0.9.3",
    "vitest": "0.18.0"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
